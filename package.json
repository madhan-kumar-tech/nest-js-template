{
  "name": "nestjs-template",
  "version": "1.0.0",
  "description": "NestJS project template with JWT, PostgreSQL, TypeORM, validation, logging, and testing.",
  "main": "dist/main.js",
  "scripts": {
    "start": "pnpm exec nest start",
    "start:dev": "pnpm exec nest start --watch",
    "build": "pnpm exec nest build",
    "lint": "pnpm exec eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm exec jest",
    "test:watch": "pnpm exec jest --watch",
    "test:e2e": "pnpm exec jest --config ./test/jest-e2e.json",
    "migration:run": "pnpm exec typeorm migration:run -d src/infrastructure/database/datasource.ts",
    "migration:revert": "pnpm exec typeorm migration:revert -d src/infrastructure/database/datasource.ts",
    "migration:generate": "pnpm exec typeorm migration:generate -d src/infrastructure/database/datasource.ts src/migrations/newMigration"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.3",
    "joi": "^17.9.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "1.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.2",
    "@types/passport-jwt": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
